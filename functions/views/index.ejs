<!DOCTYPE html>
<html lang='en'>

<head>

    <!-- Meta Tag Ref: https://gist.github.com/vnoitkumar/39a72939a8f376757e4cfeb52c9a96c8 -->

    <!-- Basic HTML Meta Tags -->
    <meta charset='UTF-8'>
    <meta http-equiv='X-UA-Compatible' content='ie=edge'>
    <meta name='viewport' content='width=device-width, initial-scale=1.0'>
    <meta http-equiv='content-type' content='text/html; charset=UTF-8'>

    <meta name=description content='<%= description %>'>
    <meta name=keywords content='<%= meta_tag_details.keywords %>'>
    <meta name='subject' content='<%= meta_tag_details.subject %>'>
    <meta name='revised' content='<%= meta_tag_details.revised %>'>
    <meta name='reply-to' content='<%= meta_tag_details.reply_to %>'>
    <meta name='url' content='<%= url %>'>
    <meta name='owner' content='<%= meta_tag_details.owner %>'>
    <meta name='geo.placename' content='<%= meta_tag_details.geo.placename %>'>
    <meta name='geo.position' content='<%= meta_tag_details.geo.position %>'>

    <!-- OpenGraph Meta Tags -->
    <meta property=og:title content='<%= title %>'>
    <meta property=og:type content='<%= meta_tag_details.open_graph.type %>'>
    <meta property=og:url content='<%= url %>'>
    <meta property=og:description content='<%= description %>'>
    <meta name='og:site_name' content='<%= meta_tag_details.open_graph.site_name %>'>
    <meta name='og:email' content='<%= meta_tag_details.open_graph.email %>'>
    <meta name='og:phone_number' content='<%= meta_tag_details.open_graph.phone_number %>'>
    <meta name='og:latitude' content='<%= meta_tag_details.open_graph.latitude %>'>
    <meta name='og:longitude' content='<%= meta_tag_details.open_graph.longitude %>'>
    <meta name='og:locality' content='<%= meta_tag_details.open_graph.locality %>'>

    <!-- Facebook App Id -->
    <meta property=fb:app_id content='<%= analytics_details.fb.app_id %>'>

    <!-- Apple Meta Tags -->
    <meta name='apple-mobile-web-app-capable' content='yes'>
    <meta name='apple-touch-fullscreen' content='yes'>
    <meta name='apple-mobile-web-app-title' content='<%= title %>'>
    <meta name='apple-mobile-web-app-status-bar-style' content='<%= styles.theme_color %>'>

    <!-- Internet Explorer Meta Tags -->
    <meta name='msapplication-TileColor' content='#ffffff'>
    <meta name='msapplication-navbutton-color' content='<%= styles.theme_color %>'>
    <meta name='msapplication-TileImage' content='/images/favicons/favicon-144.png'>
    <meta name='msapplication-config' content='/browserconfig.xml'>

    <!-- Profile Meta Tag -->
    <meta property=profile:first_name content='<%= meta_tag_details.profile.first_name %>'>
    <meta property=profile:username content='<%= meta_tag_details.profile.username %>'>
    <meta property=profile:gender content='<%= meta_tag_details.profile.gender %>'>

    <!-- Item Property Meta Tag -->
    <meta itemprop=name content='<%= title %>'>
    <meta itemprop=description content='<%= description %>'>

    <!-- OpenGraph Meta Tags - Image -->
    <meta property=og:image content='<%= meta_tag_details.social_share.fb.image %>'>
    <meta property='og:image:height' content='<%= meta_tag_details.social_share.fb.image_height %>'>
    <meta property='og:image:width' content='<%= meta_tag_details.social_share.fb.image_width %>'>
    <meta property='og:image:type' content='<%= meta_tag_details.social_share.fb.image_type %>'>

    <meta property='og:image:alt' content='<%= name %>'>

    <!-- Mobile App Meta Tag -->
    <meta name='mobile-web-app-capable' content='yes'>
    <meta name='theme-color' content='<%= styles.theme_color %>'>

    <!-- Twitter Meta Tag -->
    <meta name='twitter:card' content='summary'>
    <meta name='twitter:site' content='<%= meta_tag_details.social_share.twitter.site %>'>
    <meta name='twitter:creator' content='<%= meta_tag_details.social_share.twitter.creator %>'>
    <meta name='twitter:image' content='<%= meta_tag_details.social_share.twitter.image %>'>
    <meta name='twitter:title' content='<%= title %>'>
    <meta name='twitter:description' content='<%= description %>'>
    <meta name='twitter:image:alt' content='<%= hashtag %>'>

    <link rel='canonical' href='<%= url %>'>

    <!-- Faviconit - http://faviconit.com/en -->
    <link rel='shortcut icon' href='/images/favicons/favicon.ico'>
    <link rel='icon' sizes='16x16 32x32 64x64' href='/images/favicons/favicon.ico'>
    <link rel='icon' type='image/png' sizes='196x196' href='/images/favicons/favicon-192.png'>
    <link rel='icon' type='image/png' sizes='160x160' href='/images/favicons/favicon-160.png'>
    <link rel='icon' type='image/png' sizes='96x96' href='/images/favicons/favicon-96.png'>
    <link rel='icon' type='image/png' sizes='64x64' href='/images/favicons/favicon-64.png'>
    <link rel='icon' type='image/png' sizes='32x32' href='/images/favicons/favicon-32.png'>
    <link rel='icon' type='image/png' sizes='16x16' href='/images/favicons/favicon-16.png'>

    <link rel='apple-touch-icon' href='/images/favicons/favicon-57.png'>
    <link rel='apple-touch-icon' sizes='114x114' href='/images/favicons/favicon-114.png'>
    <link rel='apple-touch-icon' sizes='72x72' href='/images/favicons/favicon-72.png'>
    <link rel='apple-touch-icon' sizes='144x144' href='/images/favicons/favicon-144.png'>
    <link rel='apple-touch-icon' sizes='60x60' href='/images/favicons/favicon-60.png'>
    <link rel='apple-touch-icon' sizes='120x120' href='/images/favicons/favicon-120.png'>
    <link rel='apple-touch-icon' sizes='76x76' href='/images/favicons/favicon-76.png'>
    <link rel='apple-touch-icon' sizes='152x152' href='/images/favicons/favicon-152.png'>
    <link rel='apple-touch-icon' sizes='180x180' href='/images/favicons/favicon-180.png'>

    <title> <%= title %> </title>

    <link href='/css/main.css' rel='stylesheet'>
    <link href='/css/min-width-1024px.css' rel='stylesheet' media='(min-width: 1024px)'>
    <link href='/css/min-width-1200px.css' rel='stylesheet' media='(min-width: 1200px)'>
    <link href='/css/min-width-600px.css' rel='stylesheet' media='(min-width: 600px)'>
    <link href='/css/min-width-760px.css' rel='stylesheet' media='(min-width: 760px)'>

    <link href='/css/fontawesome/css/custom-fontawesome.css' rel='stylesheet'>
    <link href='https://fonts.googleapis.com/css?family=Lato:300' rel='stylesheet' crossorigin>

    <link rel='manifest' href='/manifest.json'>
    <style>
        body {
            font-family: 'Lato', sans-serif;
        }
        .sr-only {
            position:absolute;
            left:-10000px;
            top:auto;
            width:1px;
            height:1px;
            overflow:hidden;
        }
        @media (max-width:1023px) {
            .t-section__header{border-top:1px solid #404242;border-bottom:1px solid #404242}
            .t-section--header .t-section__header{border-top:none;border-bottom:none}
        }
    </style>
</head>

<body>
    <h1 class='sr-only'><%= h1 %></h1>
    <h2 class='sr-only'><%= hashtag %></h2>

    <div class='c-main-container'>
        <section class='o-section o-section--header t-section t-section--header'>
            <div class=c-header>
                <div class='o-section__header-bg t-section__header'></div>
                <div class='o-section__content-bg t-section__content'></div>
                <div class=o-container>
                    <div class=o-section__container>
                        <header class='o-section__header c-header__header t-section__header'>
                            <div class=c-header__inner-header>
                                <div class=c-header__avatar>
                                    <div class='a-header c-avatar'>
                                        <img class=c-avatar__img src='<%= avatar_image %>' alt='<%= name %>'>
                                    </div>
                                </div>
                            </div>
                        </header>
                        <div class='o-section__content c-header__content t-section__content'>
                            <div class=c-header__inner-content>
                                <div class=c-header__top>
                                    <div class=c-header__brand>
                                        <div class=c-brand>
                                            <div class='c-brand__title t-title'>
                                                <span class=c-brand__sizer>
                                                    <a target=”_blank” href='http://www.google.com/search?q=%23vnoitkumar' rel=noreferrer aria-label='hashtag' class='a-header c-brand__first-word t-title__first-word'> <%= hashtag %> </a>
                                                    <span class='a-header c-brand__second-word t-title__second-word'> <%= name %> </span>
                                                </span>
                                            </div>
                                            <h2 class='a-header c-brand__sub-title t-sub-title'>
                                                <span class=c-brand__sizer> <%= job_profile %> </span>
                                            </h2>
                                        </div>
                                    </div>
                                    <ul class='c-header__social-buttons c-social-buttons'>
                                        <% social_profiles.forEach(function(social_profile){ %>
                                            <li class=a-header>
                                                <a href='<%- social_profile.link %>' target=_blank rel=noreferrer class='c-social-button t-social-button' aria-label='<%- social_profile.name %>'>
                                                    <i class='<%- social_profile.font_awesome_icon %>'></i>
                                                </a>
                                            </li>
                                        <% }); %>
                                    </ul>
                                </div>
                                <div class=c-header__contact>
                                    <hr class='a-header c-header__contact-divider'>
                                    <div class=o-grid>
                                        <div class='o-grid__col-md-3 o-grid__col-sm-6'>
                                            <div class='a-header o-content'>
                                                <div class=o-content__body>
                                                    <h4>
                                                        <i class='fas fa-map-marker-alt'></i> &nbsp; Location
                                                    </h4>
                                                    <address> <%= current_location %> </address>
                                                </div>
                                            </div>
                                        </div>
                                        <div class='o-grid__col-md-3 o-grid__col-sm-6'>
                                            <div class='a-header o-content'>
                                                <div class=o-content__body>
                                                    <a href='tel:<%= mobile %>' class=t-link-container>
                                                        <h4>
                                                            <i class='fas fa-mobile-alt'></i> &nbsp; Mobile
                                                        </h4>
                                                        <p>
                                                            <span class=t-link-container__item--blended> <%= mobile_label %> </span>
                                                        </p>
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class='o-grid__col-md-3 o-grid__col-sm-6'>
                                            <div class='a-header o-content'>
                                                <div class=o-content__body>
                                                    <a href='mailto:<%= email %>' class=t-link-container>
                                                        <h4>
                                                            <i class='far fa-envelope'></i> &nbsp; Email
                                                        </h4>
                                                        <p>
                                                            <span class=t-link-container__item--blended> <%= email %> </span>
                                                        </p>
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class='o-grid__col-md-3 o-grid__col-sm-6'>
                                            <div class='a-header o-content'>
                                                <div class=o-content__body>
                                                    <a href='<%= resume_download_link %>' class=t-link-container>
                                                        <h4>
                                                            <i class='far fa-file-pdf'></i> &nbsp; Resume
                                                        </h4>
                                                        <p>
                                                            <span class=t-link-container__item--blended> Download </span>
                                                        </p>
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>



        <section class='o-section t-section'>
            <div class='o-section__header-bg t-section__header'></div>
            <div class='o-section__content-bg t-section__content'></div>
            <div class=o-container>
                <div class=o-section__container>
                    <header class='o-section__header t-section__header'>
                        <div class=o-content>
                            <h2 class=o-section__heading> Intro</h2>
                            <div class='o-content__body o-section__description'>What I am all about.</div>
                        </div>
                    </header>
                    <div class='o-section__content t-section__content'>
                        <div class=o-content>
                            <div class=c-intro>
                                <div class=o-content__body>
                                    <p>Web Developer, skilled in all stages of the development life cycle for a dynamic web project in startups.</p>
                                    <p>Proficient in
                                        <br> <i class='fab fa-php'></i> &nbsp; PHP
                                        <br> <i class='fab fa-laravel'></i> &nbsp; Laravel
                                        <br> <i class='fas fa-database'></i> &nbsp; MySQL
                                        <br> <i class='fab fa-html5'></i> &nbsp; HTML
                                        <br> <i class='fab fa-css3-alt'></i> &nbsp; CSS
                                        <br> <i class='fab fa-js'></i> &nbsp; JavaScript
                                        <br> <i class='fab fa-twitter'></i> &nbsp; Bootstrap
                                        <br> <i class='fas fa-code-branch'></i> &nbsp; Git
                                        <br> <i class='fab fa-aws'></i> &nbsp; AWS
                                        <br> <i class='fab fa-google'></i> &nbsp; GCP
                                        <br> <i class='fab fa-docker'></i> &nbsp; Docker
                                        <br>
                                    </p>
                                    <p>Experience working in
                                        <br> <i class='fab fa-node'></i> &nbsp; Node Js
                                        <br> <i class='fas fa-database'></i> &nbsp; NoSQL
                                        <br> <i class='fab fa-angular'></i> &nbsp; Angular
                                        <br> <i class='fas fa-fire'></i> &nbsp; Google Firebase
                                        <br>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>



        <section class='o-section t-section'>
            <div class='o-section__header-bg t-section__header'></div>
            <div class='o-section__content-bg t-section__content'></div>
            <div class=o-container>
                <div class=o-section__container>
                    <header class='o-section__header t-section__header'>
                        <div class=o-content>
                            <h2 class=o-section__heading> Experience</h2>
                            <div class='o-content__body o-section__description'>Yes. I&#39;ve been around.</div>
                        </div>
                    </header>
                    <div class='o-section__content t-section__content u-pb-0'>
                        <div class='a-experience-timeline c-timeline t-border-color'>
                            <% work_experiences.forEach(function(work_experience){ %>
                                <div class=c-timeline__item>
                                    <div class='c-timeline__point t-timeline__point t-primary-bg'></div>
                                    <div class=o-content>
                                        <div class=o-grid>
                                            <div class=o-grid__col-md-5>

                                                <div class=c-work__timeframe>
                                                    <%- work_experience.from.month %> <%- work_experience.from.year %> &ndash;
                                                    <%- work_experience.to.month %> <%- work_experience.to.year %>
                                                    <% if (work_experience.currently_working_here) { %>
                                                        Present
                                                    <% } %>
                                                </div>


                                                <h3 class=c-work__heading> <%- work_experience.company %></h3>
                                                <h4 class=c-work__title> <%- work_experience.designation %></h4>
                                                <div class=c-work__location><%- work_experience.location %></div>
                                            </div>
                                            <div class=o-grid__col-md-7>
                                                <p></p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            <% }); %>
                        </div>
                    </div>
                </div>
            </div>
        </section>



        <section class='o-section t-section'>
            <div class='o-section__header-bg t-section__header'></div>
            <div class='o-section__content-bg t-section__content'></div>
            <div class=o-container>
                <div class=o-section__container>
                    <header class='o-section__header t-section__header'>
                        <div class=o-content>
                            <h2 class=o-section__heading> Education</h2>
                            <div class='o-content__body o-section__description'></div>
                        </div>
                    </header>
                    <div class='o-section__content t-section__content u-pt-0'>
                        <div class=o-content>
                            <div class='a-education-timeline c-timeline t-border-color o-section__full-top-space'>
                                <div class='c-timeline__end t-border-color'></div>
                                <% educational_qualifications.forEach(function(educationalQualification){ %>
                                    <div class=c-timeline__item>
                                            <div class='c-timeline__point t-timeline__point t-primary-bg'></div>
                                            <div class=o-content>
                                                <div class=o-grid>
                                                    <div class=o-grid__col-md-5>
                                                        <div class=c-work__timeframe>
                                                            <% if (educationalQualification.from.month || educationalQualification.from.year) { %>
                                                                <%- educationalQualification.from.month %> <%- educationalQualification.from.year %> &ndash;
                                                            <% } %>

                                                            <% if (educationalQualification.to.month || educationalQualification.to.year) { %>
                                                                <%- educationalQualification.to.month %> <%- educationalQualification.to.year %>
                                                            <% } %>

                                                            <% if (educationalQualification.currently_studying_here) { %>
                                                                Present
                                                            <% } %>
                                                        </div>
                                                        <h3 class=c-work__heading> <%- educationalQualification.institution %></h3>
                                                        <h4 class=c-work__title> <%- educationalQualification.course %></h4>
                                                        <div class=c-work__location><%- educationalQualification.location %></div>
                                                    </div>
                                                    <div class=o-grid__col-md-7>
                                                        <p></p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                <% }); %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>



        <section class='o-section t-section o-section--footer'>
            <div class='o-section__header-bg t-section__header'></div>
            <div class='o-section__content-bg t-section__content'></div>
            <div class=o-container>
                <div class=o-section__container>
                    <header class='o-section__header t-section__header'>
                        <div class=o-content>
                            <h2 class=o-section__heading> Contact</h2>
                            <div class='o-content__body o-section__description'>Call me, maybe.</div>
                        </div>
                    </header>
                    <div class='o-section__content t-section__content'>
                        <div class=c-footer__contact>
                            <div class=o-grid>
                                <div class='o-grid__col-md-3 o-grid__col-sm-6'>
                                    <div class='a-header o-content'>
                                        <div class=o-content__body>
                                            <h4><i class='fas fa-map-marker-alt'></i> &nbsp; Location</h4>
                                            <address> <%= current_location %> </address>
                                        </div>
                                    </div>
                                </div>
                                <div class='o-grid__col-md-3 o-grid__col-sm-6'>
                                    <div class='a-header o-content'>
                                        <div class=o-content__body>
                                            <a href='tel:<%= mobile %>' class=t-link-container>
                                                <h4><i class='fas fa-mobile-alt'></i> &nbsp; Mobile</h4>
                                                <p><span class=t-link-container__item--blended> <%= mobile_label %> </span></p>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <div class='o-grid__col-md-3 o-grid__col-sm-6'>
                                    <div class='a-header o-content'>
                                        <div class=o-content__body>
                                            <a href='mailto:<%= email %>' class=t-link-container>
                                                <h4><i class='far fa-envelope'></i> &nbsp; Email</h4>
                                                <p> <span class=t-link-container__item--blended> <%= email %> </span></p>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <div class='o-grid__col-md-3 o-grid__col-sm-6'>
                                    <div class='a-header o-content'>
                                        <div class=o-content__body>
                                            <a href='<%= resume_download_link %>' class=t-link-container>
                                                <h4><i class='far fa-file-pdf'></i> &nbsp; Resume</h4>
                                                <p> <span class=t-link-container__item--blended> Download </span></p>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <hr class=c-footer__contact-divider>

                        <div class=o-content>
                            <div class=c-footer__bottom>
                                <div class=c-footer__brand>
                                    <div class=c-brand>
                                        <div class=o-content__body>
                                            <div class='c-brand__title t-title'>
                                                <span class='c-brand__sizer c-brand__sizer--sm'>
                                                    <a target=”_blank” href='http://www.google.com/search?q=%23vnoitkumar' rel=noreferrer aria-label='hashtag' class='a-footer c-brand__first-word t-title__first-word'> <%= hashtag %> </a>
                                                    <span class='a-footer c-brand__second-word t-title__second-word'> <%= name %> </span>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <ul class='c-footer__social-buttons c-social-buttons o-content__body'>
                                    <% social_profiles.forEach(function(social_profile){ %>
                                        <li class=a-footer>
                                            <a href='<%- social_profile.link %>' target=_blank rel=noreferrer class='c-social-button t-social-button' aria-label='<%- social_profile.name %>'>
                                                <i class='<%- social_profile.font_awesome_icon %>'></i>
                                            </a>
                                        </li>
                                    <% }); %>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>

    <style>.async-hide { opacity: 0 !important} </style>
    <script>(function(a,s,y,n,c,h,i,d,e){s.className+=' '+y;h.start=1*new Date;
    h.end=i=function(){s.className=s.className.replace(RegExp(' ?'+y),'')};
    (a[n]=a[n]||[]).hide=h;setTimeout(function(){i();h.end=null},c);h.timeout=c;
    })(window,document.documentElement,'async-hide','dataLayer',4000,
    {'GTM-KP9ZMS3':true});</script>
    <script>
        // Google Analytics
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
        ga('create', 'UA-127106449-1', 'auto');
        ga('require', 'GTM-KP9ZMS3');
        ga('send', 'pageview');

        // PWA
		// Check that service workers are registered
        if ('serviceWorker' in navigator) {
            // Use the window load event to keep the page load performant
            window.addEventListener('load', () => {
                navigator.serviceWorker.register('/sw.js');
            });
        }
    </script>
</body>

</html>
